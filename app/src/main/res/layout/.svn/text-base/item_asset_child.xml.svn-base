<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y33"
    android:background="@color/bffffff_13151a">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/x7_5"
        android:paddingRight="@dimen/x15_5">

        <Button
            android:id="@+id/bt_asset_deposit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y24"
            android:layout_weight="1"
            android:background="@drawable/property_btn_bg"
            android:drawableLeft="@drawable/tv_deposit_click_selector"
            android:drawablePadding="@dimen/x7"
            android:enabled="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/x23"
            android:text="@string/chong_zhi"
            android:textAllCaps="false"
            android:textColor="@color/f398155"
            android:textSize="@dimen/x12" />

        <Button
            android:id="@+id/bt_asset_withdrawal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y24"
            android:layout_weight="1"
            android:background="@drawable/property_btn_bg"
            android:drawableLeft="@drawable/tv_withdrawal_click_selector"
            android:drawablePadding="@dimen/x7"
            android:enabled="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/x23"
            android:singleLine="true"
            android:text="@string/ti_xian"
            android:textAllCaps="false"
            android:textColor="@color/f398155"
            android:textSize="@dimen/x12" />

        <Button
            android:id="@+id/bt_asset_trad"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y24"
            android:layout_weight="1"
            android:background="@drawable/property_btn_bg"
            android:drawableLeft="@drawable/tv_trad_click_selector"
            android:drawablePadding="@dimen/x7"
            android:enabled="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/x23"
            android:text="@string/jiao_yi"
            android:textAllCaps="false"
            android:textColor="@color/f398155"
            android:textSize="@dimen/x12" />

    </LinearLayout>

    <View
        android:id="@+id/group_line"
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="@dimen/x13"
        android:layout_marginRight="@dimen/x13"
        android:background="@color/be3e6e4_29ffffff"></View>
</RelativeLayout>
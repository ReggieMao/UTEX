<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <!--android:drawable="@color/b398155"-->
        <shape>
            <stroke android:width="1dp" android:color="@color/f398155"></stroke>
            <corners android:radius="100dp" />
            <solid android:color="@color/bffffff_13151a"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <!--android:drawable="@color/b419461"-->
        <shape>
            <stroke android:width="1dp" android:color="@color/b419461"></stroke>
            <corners android:radius="100dp" />
            <solid android:color="@color/bffffff_13151a"/>
        </shape>
    </item>
</selector>
<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/bf2f5f3_0a0a0a"></solid>
            <stroke android:width="1dp" android:color="@color/be3e6e4_29ffffff"></stroke>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/bffffff_13151a"></solid>
            <stroke android:width="1dp" android:color="@color/be3e6e4_29ffffff"></stroke>
        </shape>
    </item>

</selector>